{
  "version": 1,
  "projects": {
    "logger-api": {
      "root": "libs/logger/api",
      "sourceRoot": "libs/logger/api/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": [
              "libs/logger/api/tsconfig.lib.json",
              "libs/logger/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/logger/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/logger/api/jest.config.js",
            "tsConfig": "libs/logger/api/tsconfig.spec.json"
          }
        }
      }
    },
    "logger-console": {
      "root": "libs/logger/console",
      "sourceRoot": "libs/logger/console/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "tslint",
            "tsConfig": [
              "libs/logger/console/tsconfig.lib.json",
              "libs/logger/console/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/logger/console/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/logger/console/jest.config.js",
            "tsConfig": "libs/logger/console/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "defaultProject": "logger-api"
}
